<?xml version="1.0"?>
<config>
	<application>
		<framerate_cap>60</framerate_cap>
		<product name="Dungeon Brawler" />
		<organization>Angry Frogs</organization>
	</application>
	<window>
		<resolution width="1280" height="720"/>
		<fullscreen value="false" />
		<borderless value="false" />
		<resizable value="false" />
		<fullscreen_window value="false" />
		<title>Dungeon Brawler</title>
	</window>
	<filesystem>
	</filesystem>
	<input>
		<buffer window="2000"/>
		<axis tolerance="0.2"/>
	</input>
	<entities>
		<schemes>
			<default up="11" down="12" left="13" right="14" light_attack="2" heavy_attack="0" special_1="10" special_2="9" grab="1" switch="3" />
		</schemes>
	</entities>
	<audio>
		<flags flac="true" mod="false" mp3="true" ogg="true" />
		<sfx chunksize="1024" channels="16" />
	</audio>
</config>
